<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping namespace="Hd.Model.Cn" assembly="Hd.Model.Cn" xmlns="urn:nhibernate-mapping-2.2">
  <class name="换汇" table="财务_换汇" optimistic-lock="version">

<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping namespace="Hd.Model.Jk" assembly="Hd.Model.Jk" xmlns="urn:nhibernate-mapping-2.2">
  <class name="进口票过程转关标志" table="业务过程_进口票_转关标志" optimistic-lock="version">
